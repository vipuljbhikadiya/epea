{
  "$schema": "https://json.schemastore.org/block.json",
  "apiVersion": 2,
  "name": "epea-theme/cardlistingitem",
  "version": "0.1.0",
  "title": "Item",
  "category": "theme",
	"keywords": ["card", "image", "listing"],
  "description": "",
  "supports": {
    "html": true,
    "anchor": false,
    "customClassName": false
  },
  "attributes": {
    "headline": {
      "type": "string",
      "source": "html",
      "selector": ".headline",
      "default": ""
    },
    "paragraph": {
      "type": "string",
      "source": "html",
      "selector": ".text",
      "default": ""
    },
    "buttonText": {
      "type": "string",
      "source": "html",
      "selector": ".button__helper",
      "default": ""
    },
    "imageId": {
      "type": "number"
    },
    "imageAlt": {
      "type": "string",
      "default": ""
    },
    "imageDefaultAlt": {
      "type": "string",
      "default": ""
    },
    "imageUrl": {
      "type": "string"
    },
    "webpImageUrl": {
      "type": "string"
    },
    "mdimageUrl": {
      "type": "string"
    },
    "webpmdImageUrl": {
      "type": "string"
    },
    "xsimageUrl": {
      "type": "string"
    },
    "webpxsImageUrl": {
      "type": "string"
    },
    "itemLink": {
      "type": "object"
    }
  },
  "textdomain": "epea-theme",
  "parent": ["epea-theme/cardlisting"],
  "editorScript": "file:./index.js"
}
