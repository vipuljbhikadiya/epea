{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "epea-theme/form",
	"version": "0.1.5",
	"title": "Form",
	"category": "block-category-basic",
	"description": "",
	"supports": {
		"html": false,
		"anchor": false,
		"customClassName": false
	},
	"attributes": {
		"clientid": {
			"type": "string",
			"default": ""
