		"anchor": {
			"type": "string",
			"default": ""
		}
	},
	"keywords": ["form", "general", "epea-theme"],
	"parent": ["epea-theme/section", "epea-theme/sectionhero", "epea-theme/sectionherosub", "epea-theme/sectionswipe", "epea-theme/tabsslideritem"],
	"textdomain": "epea-theme",
	"editorScript": "file:./index.js",
  	"editorStyle": "file:./index.css"
}
