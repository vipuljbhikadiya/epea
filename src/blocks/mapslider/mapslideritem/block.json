{
  "$schema": "https://json.schemastore.org/block.json",
  "apiVersion": 2,
  "name": "epea-theme/mapslideritem",
  "version": "0.1.5",
  "title": "Item",
  "category": "theme",
  "description": "",
  "supports": {
    "multiple": true,
    "reusable": false,
    "html": false,
    "customClassName": false
  },
  "attributes": {
		"markerXPosition": {
			"type": "number",
			"default": 0
		},
		"markerYPosition": {
			"type": "number",
			"default": 0
		},
		"ButtonLink": {
			"type": "string",
			"default": ""
		},
    "adress": {
      "type": "string",
      "source": "html",
      "selector": ".slider-map__adress"
    },
    "contact": {
      "type": "string",
      "source": "html",
      "selector": ".slider-map__contact"
    },
		"flag": {
			"type": "string",
			"default": "germany"
		}
  },
  "textdomain": "epea-theme",
  "parent": ["epea-theme/mapslider"],
  "editorScript": "file:./index.js"
}
